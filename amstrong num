class amstrong
{
public static void main(String[] args)
{
int n=123;
int temp1=n;
int temp2=n;
int count=0;
int ld=0;
int sum=0;
while(n<0)
{
count++;
n=n/10;
}
while(temp1>0)
{
ld=temp1%10;
int pr=1;
for(int i=1;i<=count;i++)
{
pr=pr*ld;
}
sum=sum+pr;
temp1=temp1/10;
}
if(temp2==n)
{
System.out.println("amstrong num");
}
else
{
System.out.println("not a armstrong num");
}
}
}
