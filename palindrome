class palindrome
{
public static void main(String[] args)
{
int n=121;
int k=n;
int temp=0;
int reverse=0;
while(n>0)
{
temp=n%10;
reverse=(reverse*10)+temp;
n=n/10;
}
System.out.println(reverse);
if(k==reverse)
{
System.out.println("given num is palindrome");
}
else
{
System.out.println("given num is not a palindrome");
}
}
}
