class prime
{
	public static void main(String[] args) 
	{
		int n=6;
		int count=0;
		for(int i=1;i<=n;i++);
		{
			if(n%i==0)
			{
			     count++;
			}
		}
		System.out.println(count);
		if(count==2)
		{
			System.out.println("palindrome");
		}
		else
		{
			System.out.println("not a palindrome");
		}
		
	}
}
