class lcm 
{
	public static void main(String[] args) 
	{
		int n=16;//8
		int m=14;//7
		int temp=1;//1//2
		int lcm=1;
		
		
            
				

			for(int i=1;i<=n&&i<=m;i++)
		    {
				if(n%i==0 && m%i==0)
			    {
					temp=i;
				
			    }
				
				
				

			}
			System.out.println(temp);
			lcm=(m*n)/temp;
			System.out.println(lcm);
	}
				
			
			
	}
			
				
